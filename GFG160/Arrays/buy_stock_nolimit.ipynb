    "def maximumprofit(arr):\n",
    "    res= 0\n",
    "    for i in range(1,len(arr)):\n",
    "        if arr[i] >arr[i-1]:\n",
    "            res+= arr[i]-arr[i-1]\n",
    "    return res\n",
    "             \n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    arr= [7,1,5,3,6,4]\n",
    "    res= maximumprofit(arr)\n",
    "    print(res,end=' ')\n",
    "    "
