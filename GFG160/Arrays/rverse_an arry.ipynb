    "def rverse_array(n):\n",
    "    i = 0\n",
    "    j = len(n)-1\n",
    "    while i < j :\n",
    "        n[i],n[j]= n[j],n[i]\n",
    "        i+=1\n",
    "        j-=1\n",
    "    return n \n",
    "\n",
    "\n",
    "\n",
    "n= [1,2,3,4,78,6] \n",
    "rverse_array(n)"
   ]
