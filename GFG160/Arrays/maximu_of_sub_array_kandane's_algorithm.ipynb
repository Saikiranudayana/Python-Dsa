    "def max_sum_of_sub_array(arr):\n",
    "    maxending = arr[0]\n",
    "    res = arr[0]\n",
    "    for i in range(1,len(arr)):\n",
    "        maxending = max(maxending+arr[i],arr[i])\n",
    "        \n",
    "        res= max(res,maxending)\n",
    "        \n",
    "    return res\n",
    "\n",
    "arr = [2, 3, -8, 7, -1, 2, 3]\n",
    "max_sum_of_sub_array(arr)"
   ]
