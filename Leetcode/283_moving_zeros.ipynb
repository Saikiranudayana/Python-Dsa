    "#283\n",
    "def moving_zeros(arr):\n",
    "    count = 0\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] != 0:\n",
    "            arr[i],arr[count]= arr[count],arr[i]\n",
    "            count+= 1\n",
    "    return arr \n",
    "\n",
    "arr = [0,1,0,3,12]\n",
    "moving_zeros(arr)"
   ]
