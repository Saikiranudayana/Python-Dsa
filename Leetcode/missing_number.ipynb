    "##268\n",
    "def missing_value(arr):\n",
    "    arr.sort()\n",
    "    for i,v in enumerate(arr):\n",
    "        if i !=v:\n",
    "            return v-1\n",
    "        if v==len(arr)-1:\n",
    "            return v+1\n",
    "\n",
    "arr = [1,0,3]\n",
    "missing_value(arr)"
   ]
