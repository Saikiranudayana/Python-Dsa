   "source": [
    "def darr(nums):\n",
    "    n = len(nums)\n",
    "   \n",
    "    result= 2*([0]*n)\n",
    "    \n",
    "    for i in range(n):\n",
    "        result[i]=nums[i]\n",
    "        result[i+n]= nums[i]\n",
    "    return result\n",
    "\n",
    "nums= [1,2,1]\n",
    "darr(nums)\n"
   ]
