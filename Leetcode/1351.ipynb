    "def count_Negative(arr):\n",
    "    count= 0\n",
    "    \n",
    "    for i in arr:\n",
    "        for j in i:\n",
    "            if  j<0 :\n",
    "                count+=1\n",
    "    return count\n",
    "        \n",
    "\n",
    "if __name__ == '__main__':\n",
    "    arr = [[4,3,2,-1],[3,2,1,-1],[1,1,-1,-2],[-1,-1,-2,-3]]\n",
    "    print(count_Negative(arr))"
   ]
