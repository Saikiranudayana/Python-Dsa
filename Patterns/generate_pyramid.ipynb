    "def generate_pyramid(n):\n",
    "    output= []\n",
    "    for i in range(n,n+1):\n",
    "        if i==1:\n",
    "       output.append(' '*(n-i)+'*'*(2*i-1)+' '*(n-1))\n",
    "       \n",
    "    \n",
    "    return output\n",
    "     \n",
    "\n",
    "n= int(input(\"Eter a number\"))\n",
    "generate_pyramid(n)"
   ]
