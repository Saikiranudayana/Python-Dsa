   "source": [
    "def hallow_inverted_right(n):\n",
    "    output = []\n",
    "    for i in range(n,0,-1):\n",
    "        if i==1 or i== n:\n",
    "            output.append('*'*i)\n",
    "        else:\n",
    "            output.append('*'+' '*(i-2)+'*')\n",
    "    return output\n",
    "\n",
    "n = int(input())\n",
    "hallow_inverted_right(n)"
   ]
