   "source": [
    "def generate_daimond(n):\n",
    "    output = []\n",
    "    for i in range(1,n+1):\n",
    "        output.append(' '*(n-i)+'*'*(2*i -1)+' '*(n-i))\n",
    "        \n",
    "    for i in range(n,0,-1):\n",
    "        output.append(' '*(n-i)+'*'*(2*i -1)+' '*(n-i))    \n",
    "    return output\n",
    "\n",
    "n= int(input(\"Number\"))\n",
    "generate_daimond(n)"
   ]
