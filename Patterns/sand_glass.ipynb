    "def sand_glass(n):\n",
    "    output= []\n",
    "    for i in range(n):\n",
    "        output.append(' '*(i)+'*'*(2*(n-i)-1)+' '*(i))\n",
    "        \n",
    "    for i in range(n-1):\n",
    "        output.append(' '*(n-i-2)+'*'*(2*(i+1)+1)+' '*(n-i-2))\n",
    "    return output\n",
    "\n",
    "n= int(input())\n",
    "sand_glass(n)"
   ]
