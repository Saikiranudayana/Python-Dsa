    "def hallow_right_angle(n):\n",
    "    output= []\n",
    "    for i in range(1,n):\n",
    "        if i==n or i==1:\n",
    "            output.append('*'*i)\n",
    "        else:\n",
    "            output.append('*'+' '*(i-2)+'*')\n",
    "    return output\n",
    "n= int(input()) \n",
    "hallow_right_angle(n)"
   ]
