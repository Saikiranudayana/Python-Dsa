    "def lengthLL(head):\n",
    "    temp =head \n",
    "    ans= 0\n",
    "    while temp!=None:\n",
    "        temp =temp.next\n",
    "        ans = ans+1\n",
    "    return ans\n",
    "\n",
    "\n",
    "head = take_input_better()\n",
    "\n",
    "length = lengthLL(head)\n",
    "print(length)"
