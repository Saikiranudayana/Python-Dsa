   "source": [
    "def inverted_pattern(n):\n",
    "    output= []\n",
    "    for i in range(n,0,-1):\n",
    "        a= ' '*(n-i)+'*'*(2*i-1)+' '*(n-i)\n",
    "        output.append(a)\n",
    "    return output\n",
    "\n",
    "n= int(input(\"num\"))\n",
    "inverted_pattern(n)"
   ]
