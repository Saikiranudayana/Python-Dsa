    "def right_angle_triangle(n):\n",
    "    output= []\n",
    "    \n",
    "    for j in range(1,n+1):\n",
    "        output.append(str(j)*j)\n",
    "    return output\n",
    " \n",
    "n= int(input(\"Enter num\"))\n",
    "right_angle_triangle(n)"
