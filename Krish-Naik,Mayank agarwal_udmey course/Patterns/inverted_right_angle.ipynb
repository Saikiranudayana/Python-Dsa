    "def inverted_right_angle(n):\n",
    "    output= []\n",
    "    \n",
    "    for i in range(n,):\n",
    "        if i==0:\n",
    "            output.append('*'*n)\n",
    "        else:\n",
    "            output.append('*'*(n-i))\n",
    "    return output\n",
    "    \n",
    "    \n",
    "n= int(input(\"Num\"))\n",
    "\n",
    "inverted_right_angle(n)"
   ]
