    "##Update all indices in a global  list\n",
    "anslist=[]\n",
    "def updateallindices(l,x,index):\n",
    "    if len(l)==index:\n",
    "        return\n",
    "    updateallindices(l,x,index+1)\n",
    "    if (l[index]==x):\n",
    "        anslist.append(index)\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "updateallindices([3,2,5,2,8,2,1],2,0)\n",
    "print(anslist)\n",
    "\n"
   ]
