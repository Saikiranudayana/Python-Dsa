   "source": [
    "def returnallindices(l,x,index):\n",
    "    if len(l)==0:\n",
    "        return []\n",
    "    smalllist = returnallindices(l,x,index+1)\n",
    "    if (l[index]==x):\n",
    "        smalllist.insert(0,index)\n",
    "    return smalllist\n",
    "    \n",
    "\n",
    "ans = returnallindices([3,2,5,2,0,2,1],2,0)\n",
    "print(ans)"
