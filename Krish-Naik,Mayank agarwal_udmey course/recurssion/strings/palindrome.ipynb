   "source": [
    "def palindromehelper(s,start,end):\n",
    "    if start>=end:\n",
    "        return True\n",
    "    if s[start] !=s[end]:\n",
    "        return False\n",
    "    return palindromehelper(s,start+1,end-1)\n",
    "        \n",
    "def palindrome(s):\n",
    "    return palindromehelper(s,0,len(s)-1)\n",
    "     \n",
    "\n",
    "s= 'nitin'\n",
    "print(palindrome(s))"
   ]
