   "source": [
    "def subsequences(s):\n",
    "    if s=='':\n",
    "        ans = ['']\n",
    "        return ans\n",
    "    smallans = subsequences(s[1:])\n",
    "    mychar= s[0]\n",
    "    ans = []\n",
    "    ans.extend(smallans)\n",
    "    \n",
    "    for i in smallans:\n",
    "        ans.append(mychar+i)\n",
    "    return ans\n",
    "\n",
    "     \n",
    "\n",
    "s= 'abc'\n",
    "l= subsequences(s)\n",
    "print(l)"
   ]
