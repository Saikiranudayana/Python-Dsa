    "def number_of_digits(n):\n",
    "    if (n>=1 and n<=9):\n",
    "        return 1\n",
    "    if (n==0):\n",
    "        return 1\n",
    "    smallnumber = int(n/10)\n",
    "    smallans = number_of_digits(smallnumber)\n",
    "\n",
    "    num = 1 +smallans\n",
    "    return num\n",
    "    \n",
    "\n",
    "print(number_of_digits(123))"
   ]
